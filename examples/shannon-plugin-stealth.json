{
  "shannon": {
    "require_authorization": false,
    "docker_enabled": true,
    "docker_image": "kalilinux/kali-rolling",
    "real_time_notifications": true,
    "stealth_mode": true,
    "continuous_scan": false,
    "report_format": "markdown",
    "max_parallel_phases": 2,
    "timeout_per_phase_minutes": 120
