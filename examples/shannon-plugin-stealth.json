  "agents": {
    "shannon": {
      "model": "anthropic/claude-opus-4-6",
      "temperature": 0.05,
      "max_output_tokens": 64000
    },
    "shannon-recon": {
      "model": "anthropic/claude-opus-4-6",
      "temperature": 0.05
    },
    "shannon-exploit": {
      "model": "anthropic/claude-opus-4-6",
      "temperature": 0.05
    },
    "shannon-report": {
      "model": "anthropic/claude-opus-4-6",
      "temperature": 0.1
    }
  }
}
